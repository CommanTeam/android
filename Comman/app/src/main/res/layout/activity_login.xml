<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_login"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.user.LoginActivity"
    android:background="@color/primaryColor"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/login_splash_stop"
        android:layout_width="330dp"
        android:layout_height="640dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/img_97" />


    <com.kakao.usermgmt.LoginButton
        android:id="@+id/login_kakaoLogin_btn"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:layout_alignBottom="@+id/login_splash_stop"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="112dp"></com.kakao.usermgmt.LoginButton>



</RelativeLayout>
